{
  "printWidth": 100,
  "useTabs": true,
  "semi": true,
  "singleQuote": true,
  "quoteProps": "consistent",
  "jsxSingleQuote": false,
  "trailingComma": "all",
  "bracketSpacing": true,
  "jsxBracketSameLine": true,
  "arrowParens": "always",
  "endOfLine": "lf",

  "overrides": [
    {
      "files": "package.json",
      "options": {
        "tabWidth": 2,
        "useTabs": false
      }
    }
  ]
}
